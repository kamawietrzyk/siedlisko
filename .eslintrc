{
  "extends": [
    "standard",
    "plugin:react/recommended",
    "plugin:css-modules/recommended",
    "plugin:cypress/recommended"
  ],
  "plugins": [
    "react",
    "css-modules"
  ],
  "rules": {
    "react/prop-types": "off",
    "no-console": ["warn", {"allow": ["error", "info"]}],
    "react/display-name": "off"

  },
  "parser": "babel-eslint",
  "settings": {
    "react": {
      "version": "16.4.9"
    }
  }
}
